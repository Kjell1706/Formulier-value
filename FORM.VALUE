#personen{  
    font-family: Arial,Helvetica,sans-serif,verdana;
    font-size: 12px;
    text-decoration: none solid rgb(0,0,0);
    white-space: pre;
    word-spacing: 0px;
    background-color: white;
    color: black;
    height: 35px;
    width: 266px;
    padding: 0 0 0 10px;
    margin-top: 4%;
    margin-left: 5%;
    position: relative;
    top: 0px;
    bottom: 0px;
    right: 0px;
    left: 0px;
    display: inline-block;
    z-index: 4;
    cursor: default;
}  



